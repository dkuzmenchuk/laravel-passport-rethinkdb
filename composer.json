{
    "name": "dkuzmenchuk/laravel-passport-rethinkdb",
    "description": "Laravel Passport RethinkDB provides OAuth2 server support to Laravel with RethinkDB adapter.",
    "keywords": ["laravel", "passport", "oauth", "rethinkdb"],
    "license": "MIT",
    "authors": [
        {
            "name": "Denis Kuzmenchuk",
            "email": "denis.kuzm.stans@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "firebase/php-jwt": "~3.0|~4.0",
        "guzzlehttp/guzzle": "~6.0",
        "illuminate/auth": "~5.4",
        "illuminate/console": "~5.4",
        "illuminate/encryption": "~5.4",
        "illuminate/http": "~5.4",
        "league/oauth2-server": "~5.0",
        "symfony/psr-http-message-bridge": "~1.0",
        "zendframework/zend-diactoros": "~1.0",
        "phpseclib/phpseclib": "^2.0",
        "dkuzmenchuk/laravel-rethinkdb": "^3.0"
    },
    "require-dev": {
        "mockery/mockery": "~0.9",
        "phpunit/phpunit": "~5.0"
    },
    "autoload": {
        "psr-4": {
            "dkuzmenchuk\\PassportRethinkdb\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
